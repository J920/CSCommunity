<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="376dp"
        android:layout_height="559dp"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:elevation="3dp"
        card_view:cardCornerRadius="@dimen/card_album_radius">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txt"
                android:layout_width="match_parent"
                android:layout_height="519dp"
                android:layout_margin="4dp"
                android:layout_marginTop="8dp"
                android:paddingLeft="5dp"
                android:textColor="@color/black"
                android:textSize="25dp"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintHorizontal_bias="0.0"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="1.0" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="76dp"
                android:layout_height="85dp"
                android:layout_marginStart="20dp"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:srcCompat="@color/black" />

            <android.support.v7.widget.CardView
                android:layout_width="247dp"
                android:layout_height="40dp"
                android:layout_marginEnd="16dp"
                card_view:cardBackgroundColor="@color/album_title"
                card_view:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="278dp">

            </android.support.v7.widget.CardView>

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="76dp"
                android:layout_height="85dp"
                android:layout_marginStart="20dp"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.733"
                card_view:srcCompat="@color/black" />

            <android.support.v7.widget.CardView
                android:layout_width="247dp"
                android:layout_height="40dp"
                android:layout_marginEnd="16dp"
                card_view:cardBackgroundColor="@color/album_title"
                card_view:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="387dp">

            </android.support.v7.widget.CardView>

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="76dp"
                android:layout_height="85dp"
                android:layout_marginStart="20dp"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.965"
                card_view:srcCompat="@color/black" />

            <android.support.v7.widget.CardView
                android:layout_width="247dp"
                android:layout_height="40dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="28dp"
                card_view:cardBackgroundColor="@color/album_title"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="76dp"
                android:layout_height="85dp"
                android:layout_marginStart="16dp"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.23"
                card_view:srcCompat="@color/black" />

            <android.support.v7.widget.CardView
                android:layout_width="255dp"
                android:layout_height="40dp"
                card_view:cardBackgroundColor="@color/album_title"
                tools:layout_editor_absoluteX="97dp"
                tools:layout_editor_absoluteY="139dp" >

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="364dp"
                android:layout_height="39dp"
                android:background="@color/colorsBack"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintHorizontal_bias="1.0"
                card_view:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="58dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="139dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="60dp"
                android:text="أعلى النقاط خلال اسبوع"
                android:textAlignment="center"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintHorizontal_bias="0.502"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar2"
                android:layout_width="388dp"
                android:layout_height="5dp"
                android:background="@color/colorBack"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="202dp" />

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar3"
                android:layout_width="388dp"
                android:layout_height="5dp"
                android:background="@color/colorBack"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintHorizontal_bias="1.0"
                card_view:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="547dp" />


        </android.support.constraint.ConstraintLayout>

    </android.support.v7.widget.CardView>

</RelativeLayout>